<Window x:Class="NoteBook.PickDateForImport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoteBook"
        mc:Ignorable="d"
        Title="PickDateForImport" Height="178.5" Width="365">
    <Grid>
        <DatePicker x:Name="FromDate" HorizontalAlignment="Left" Margin="37,73,0,0" VerticalAlignment="Top" CalendarClosed="FromDate_CalendarClosed"/>
        <DatePicker x:Name="ToDate" HorizontalAlignment="Left" Margin="188,73,0,0" VerticalAlignment="Top" CalendarClosed="ToDate_CalendarClosed"/>
        <Label Content="Выберите записи с какого интервала&#xD;&#xA;времени импортировать" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="600" FontSize="20" FontFamily="Segoe UI Semilight"/>
        <Label Content="С" HorizontalAlignment="Left" Margin="15,73,0,0" VerticalAlignment="Top" Height="24"/>
        <Label Content="По" HorizontalAlignment="Left" Margin="157,73,0,0" VerticalAlignment="Top" Height="24"/>
        <Button x:Name="OK_btn" Content="Ok" HorizontalAlignment="Left" Margin="15,109,0,0" VerticalAlignment="Top" Width="325" Click="OK_btn_Click"/>

    </Grid>
</Window>
